<header class="site-header nav-container">
  <div class="nav-wrapper grid-container">
    <div class="grid-row">

      <div class="header-logo grid-span4">
        Logo
      </div>
      <div class="header-nav grid-span8">
        <ul class="nav-list">
          <% data.navigation.items.each do |nav| %>
            <li class="nav-item">
              <%= link_to nav.name, nav.url, class: 'header-nav-link' %>
            </li>
          <% end %>
          </ul>
      </div>

    </div>
  </div>
</header>